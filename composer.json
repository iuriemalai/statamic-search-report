{
    "name": "iuriemalai/statamic-search-report",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stillat\\StatamicSearchReport\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "statamic/cms": "^4.0 || ^5 || ^6",
        "stillat/statamic-template-resolver": "^1.1"
    },
    "extra": {
        "statamic": {
            "name": "Search Report for Statamic",
            "description": "Provides a simple report of search queries performed on your Statamic site."
        },
        "laravel": {
            "providers": [
                "Stillat\\StatamicSearchReport\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
